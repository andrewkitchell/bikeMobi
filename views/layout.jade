<!DOCTYPE html>
doctype 5

html(ng-app="myApp", ng-cloak)
  head
    meta(charset='utf8')
    base(href='/')
    title Bike
    link(rel="shortcut icon", href="/images/favicon.png")
    script(src='http://code.jquery.com/jquery-2.0.3.js')
    script(type="text/javascript", src="https://js.stripe.com/v2/")
    script(src="https://checkout.stripe.com/checkout.js")
    link(rel='stylesheet', href='https://netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css')
    link(rel='stylesheet', href='//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css')
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800|Open+Sans+Condensed:300', rel='stylesheet', type='text/css')
    link(href='http://fonts.googleapis.com/css?family=Rouge+Script', rel='stylesheet', type='text/css')
    link(rel='stylesheet', href='/css/app.css')
    link(rel='stylesheet', href='/css/free.css')
    link(rel='stylesheet', href='/css/store.css')

  body(ng-cloak)

    navlist(ng-controller="NavCtrl")

    block body

    div.footer(ng-cloak)
     ul.footer-list
      a
       li
        span help@bikedoctor.com
      a(href="www.facebook.com/bikedoctor")
       li
        i.fa.fa-facebook-square
      a(href="www.twitter.com/bikedoctor")
       li
        i.fa.fa-twitter-square
